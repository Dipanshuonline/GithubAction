            <stringProp name="script">import java.text.SimpleDateFormat;
import java.util.Date;

// Retrieve the long value from the JMeter variable
String longValueAsString = vars.get(&quot;START&quot;);
long StartTime = Long.parseLong(longValueAsString);

// Start time
long EndTime = System.currentTimeMillis();
long TimeForEachIteration=30000/3;//${Duration}/${Samples};
log.info(&quot;Transaction Time (ms): &quot; + EndTime-StartTime);
if(TimeForEachIteration&gt;(EndTime-StartTime)){
	Thread.sleep(TimeForEachIteration-(EndTime-StartTime));
}





